{
    "name": "Web Maven",
    "description" : "Share bookmarks with your friends and rate websites!",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": [
        "tabs",
        "<all_urls>"
    ],
    "action": {
        "default_popup":"popup.html",
        "default_icon": "web.png"
    }
}